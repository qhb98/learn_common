int c[1..5][1..5] = 
[[3,8,2,10,3],[8,7,2,9,7],
[6,4,2,9,7],[6,4,2,3,5],
[9,10,6,9,10]];

dvar boolean x[1..5][1..5];
minimize sum(i in 1..5) sum(j in 1..5) c[i][j] * x[i][j];

subject to {
  forall(j in 1..5)
    sum(i in 1..5) x[i][j] ==1;
  forall(i in 1..5)
    sum(j in 1..5) x[i][j] == 1;
  }